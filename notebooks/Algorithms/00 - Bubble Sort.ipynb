   "source": [
    "a = [2, 6, 3, 4, 1, 4, 6, 7, 2, 8, 9, 2]\n",
    "\n",
    "def bubblesort(a):\n",
    "    for i in range(len(a)-1, 0, -1):\n",
    "        for j in range(i):\n",
    "            if a[j] > a[j+1]:\n",
    "                temp = a[j]\n",
    "                a[j] = a[j+1]\n",
    "                a[j+1] = temp\n",
    "    return a\n",
    "\n",
    "print(bubblesort(a))"
   ]
